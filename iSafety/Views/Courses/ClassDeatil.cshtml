@{
    ViewBag.Title = "ClassDeatil";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@*<h2>ClassDeatil</h2>*@
<div id="wrapper">
    <div class="content animate-panel">
        <div class="row">
            <div class="col-lg-12">
                <div class="hpanel">
                    <div class="panel-body">
                        <div class="row">
                            <div class="form-group col-lg-12">
                                <label>Class</label>
                                <input type="text" value="" id="" class="form-control" name="Class" placeholder="Class">
                            </div>
                            <div class="form-group col-lg-12">
                                <label>Date</label>
                                <input type="date" value="" id="" class="form-control" name="" placeholder="Date" name="Date">
                            </div>
                            <div class="form-group col-lg-6">
                                <label>Start Time</label>
                                <input type="time" value="" id="" class="form-control" name="" placeholder="Start Time" name="Starttime">
                            </div>
                            <div class="form-group col-lg-6">
                                <label>End Time</label>
                                <input type="time" value="" id="" class="form-control" name="" placeholder="End Time" name="EndTime">
                            </div>
                            <div class="form-group col-lg-8">
                                <label>Instructors</label>
                                <input type="text" value="" id="" class="form-control" name="" name="Instructors" placeholder="Instructors">
                            </div>
                            <div class="form-group col-lg-4">
                                <label></label>
                                <button type="button" id="btnAddInstructors" class="btn btn-primary" data-toggle="modal" data-target="#myModal1">Add Instructors</button>
                            </div>
                            <div class="form-group col-lg-8">
                                <label>Participants</label>
                                <input type="text" value="" id="" class="form-control" name="" name="Participants" placeholder="Participants">
                            </div>
                            <div class="form-group col-lg-4">
                                <label></label>
                                <button type="button" id="btnAddParticipants" class="btn btn-primary" data-toggle="modal" data-target="#myModal1">Add Participants</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
